{
  "appConfig": {
    "sourceConfig": {
      "AadClientId": "<AAD CLIENT ID>",
      "AadSecret": "<AAD SECRET>",
      "AadTenantDomain": "<AAD TENANT DOMAIN>",
      "AadTenantId": "<AAD TENANT ID>",
      "AccountName": "<AMS ACCOUNT NAME>",
      "Location": "<AMS ACCONT LOCATION>",
      "ResourceGroup": "<AMS RESOURCE GROUP>",
      "SubscriptionId": "<AZURE SUBSCRIPTION ID>",
      "ArmAadAudience": "https://management.core.windows.net",
      "ArmEndpoint": "https://management.azure.com",
      "StorageAccountName": "<STORAGE ACCOUNT NAME>",
      "StorageAccountKey": "<STORAGE ACCOUNT KEY>",
      "StorageAccountUrl": "https://<STORAGE ACCOUNT NAME>.blob.core.windows.net/",
      "AADSettings": "Azure" //Possible values: Azure, AzureChina, AzureUSGovernment
    },
    "destinationConfig": {
      "AadClientId": "<AAD CLIENT ID>",
      "AadSecret": "<AAD SECRET>",
      "AadTenantDomain": "<AAD TENANT DOMAIN>",
      "AadTenantId": "<AAD TENANT ID>",
      "AccountName": "<AMS ACCOUNT NAME>",
      "Location": "<AMS ACCONT LOCATION>",
      "ResourceGroup": "<AMS RESOURCE GROUP>",
      "SubscriptionId": "<AZURE SUBSCRIPTION ID>",
      "ArmAadAudience": "https://management.core.windows.net",
      "ArmEndpoint": "https://management.azure.com",
      "StorageAccountName": "<STORAGE ACCOUNT NAME>",
      "StorageAccountKey": "<STORAGE ACCOUNT KEY>",
      "StorageAccountUrl": "https://<STORAGE ACCOUNT NAME>.blob.core.windows.net/",
      "AADSettings": "Azure" //Possible values: Azure, AzureChina, AzureUSGovernment
    },
    "miscellaneous": {
      //Possible values: true, false.
      //False will copy assets between DCs directly. true: will use download/upload mechanisim which depend on your network speed and may need time, but it could be helpful in case the DCs do not have a direct access, for example, copy from a private/isolated DC into public DC
      "CopyUsingLocalNetwork": "false",
      "CopyUsingAzCopy": "false"
    }
  }
}
